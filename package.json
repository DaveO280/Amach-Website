{
  "name": "amach-health",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\"",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "rebuild": "pnpm clean && pnpm install && pnpm build",
    "dev:clean": "pnpm clean:windows && pnpm dev",
    "build:clean": "rimraf .next && pnpm build",
    "start:clean": "rimraf .next && pnpm build && pnpm start",
    "clean:windows": "rimraf --no-glob .next && rimraf --no-glob node_modules/.cache && pnpm rebuild"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@shadcn/ui": "^0.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "file-saver": "^2.0.5",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "10.0.1",
    "eslint": "^9.20.1",
    "eslint-config-next": "15.1.7",
    "husky": "^8.0.0",
    "lint-staged": "^15.4.3",
    "postcss": "8.4.31",
    "rimraf": "^5.0.10",
    "tailwindcss": "3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm eslint --fix",
      "pnpm prettier --write",
      "pnpm type-check"
    ]
  }
}
