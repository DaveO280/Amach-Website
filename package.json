{
  "name": "amach-health",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm lint && pnpm type-check && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\"",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "rebuild": "pnpm clean && pnpm install && pnpm build",
    "dev:clean": "pnpm clean:windows && pnpm dev",
    "build:clean": "rimraf .next && pnpm install && pnpm build",
    "build:prod": "rimraf .next && pnpm install --prod && pnpm build",
    "start:clean": "rimraf .next && pnpm build && pnpm start",
    "clean:windows": "rimraf --no-glob .next && rimraf --no-glob node_modules/.cache && pnpm rebuild",
    "pre-commit": "lint-staged && pnpm type-check"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@shadcn/ui": "^0.0.4",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/jszip": "^3.4.1",
    "@types/papaparse": "^5.3.15",
    "axios": "^1.8.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.4.13",
    "jszip": "^3.10.1",
    "lucide-react": "^0.475.0",
    "next": "^15.1.7",
    "papaparse": "^5.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/axios": "^0.14.4",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.17.19",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "10.0.1",
    "beasties": "^0.2.0",
    "beasties-webpack-plugin": "^0.2.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.1.1",
    "eslint-define-config": "^2.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "8.4.31",
    "prettier": "^3.5.1",
    "rimraf": "^5.0.10",
    "tailwindcss": "3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm prettier --write",
      "pnpm eslint --fix"
    ],
    "**/*.{js,jsx,json,css,scss,md}": [
      "pnpm prettier --write"
    ]
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/package.json"
      ],
      "url": "./package-schema.json"
    }
  ],
  "files": [
    "src",
    "my-health-app"
  ]
}
