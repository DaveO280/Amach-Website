{
  "name": "amach-health",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm lint && pnpm type-check && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "rebuild": "pnpm clean && pnpm install && pnpm build",
    "dev:clean": "pnpm clean:windows && pnpm dev",
    "build:clean": "rimraf .next && pnpm install && pnpm build",
    "build:prod": "rimraf .next && pnpm install --prod && pnpm build",
    "start:clean": "rimraf .next && pnpm build && pnpm start",
    "clean:windows": "rimraf --no-glob .next && rimraf --no-glob node_modules/.cache && pnpm rebuild",
    "pre-commit": "lint-staged && pnpm type-check"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@shadcn/ui": "^0.0.4",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/jszip": "^3.4.1",
    "@types/papaparse": "^5.3.15",
    "axios": "^1.8.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.4.13",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "next": "15.1.7",
    "next-themes": "^0.2.1",
    "papaparse": "^5.5.2",
    "react": "^18",
    "react-dom": "^18",
    "react-error-boundary": "^5.0.0",
    "recharts": "^2.12.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/axios": "^0.14.4",
    "@types/d3-array": "^3.2.1",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-path": "^3.1.1",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/d3-timer": "^3.0.2",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/offscreencanvas": "^2019.7.3",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.0.1",
    "beasties": "^0.2.0",
    "beasties-webpack-plugin": "^0.2.0",
    "eslint": "^8",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.10",
    "tailwindcss": "3.3.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "next": "15.1.7"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm prettier --write",
      "pnpm eslint --fix"
    ],
    "**/*.{js,jsx,json,css,scss,md}": [
      "pnpm prettier --write"
    ]
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/package.json"
      ],
      "url": "./package-schema.json"
    }
  ],
  "files": [
    "src",
    "my-health-app"
  ]
}
